package com.hisun.bean.mess;

public class MessInfo {
	private String mess_id;  //消息id
	private String mess_type; //消息类型
	private String opt_name;//操作人
	private String start_time;//开始时间
	private String end_time;//结束时间
	private String mess_remarks;//操作说明
	private String mess_status;//消息状态 是否操作完
	private String mess_date;//消息日期
	private String mess_delstatus;//删除标记
	private String event_id;//事件编号
	
	public String getMess_delstatus() {
		return mess_delstatus;
	}
	public void setMess_delstatus(String mess_delstatus) {
		this.mess_delstatus = mess_delstatus;
	}
	public String getMess_id() {
		return mess_id;
	}
	public void setMess_id(String mess_id) {
		this.mess_id = mess_id;
	}
	public String getMess_type() {
		return mess_type;
	}
	public void setMess_type(String mess_type) {
		this.mess_type = mess_type;
	}
	public String getOpt_name() {
		return opt_name;
	}
	public void setOpt_name(String opt_name) {
		this.opt_name = opt_name;
	}
	public String getStart_time() {
		return start_time;
	}
	public void setStart_time(String start_time) {
		this.start_time = start_time;
	}
	public String getEnd_time() {
		return end_time;
	}
	public void setEnd_time(String end_time) {
		this.end_time = end_time;
	}
	public String getMess_remarks() {
		return mess_remarks;
	}
	public void setMess_remarks(String mess_remarks) {
		this.mess_remarks = mess_remarks;
	}
	public String getMess_status() {
		return mess_status;
	}
	public void setMess_status(String mess_status) {
		this.mess_status = mess_status;
	}

	public String getMess_date() {
		return mess_date;
	}
	public void setMess_date(String mess_date) {
		this.mess_date = mess_date;
	}
	public String getEvent_id() {
		return event_id;
	}
	public void setEvent_id(String event_id) {
		this.event_id = event_id;
	}
	

	

	
	
	public MessInfo(String mess_id, String mess_type, String opt_name,
			String start_time, String end_time, String mess_remarks,
			String mess_status, String mess_date, String mess_delstatus,
			String event_id) {
		super();
		this.mess_id = mess_id;
		this.mess_type = mess_type;
		this.opt_name = opt_name;
		this.start_time = start_time;
		this.end_time = end_time;
		this.mess_remarks = mess_remarks;
		this.mess_status = mess_status;
		this.mess_date = mess_date;
		this.mess_delstatus = mess_delstatus;
		this.event_id = event_id;
	}
	
	@Override
	public String toString() {
		return "MessInfo [mess_id=" + mess_id + ", mess_type=" + mess_type
				+ ", opt_name=" + opt_name + ", start_time=" + start_time
				+ ", end_time=" + end_time + ", mess_remarks=" + mess_remarks
				+ ", mess_status=" + mess_status + ", mess_date=" + mess_date
				+ ", mess_delstatus=" + mess_delstatus + ", event_id="
				+ event_id + "]";
	}
	public MessInfo() {
		super();
		// TODO Auto-generated constructor stub
	}
}
